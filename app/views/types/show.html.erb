<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @type.name %>
</p>

<p>
  <b>Details:</b>
  <%= @type.details %>
</p>

<h2>Workouts</h2>
<% @type.workouts.each do |workout| %>
  <p>
    <b>Name:</b>
    <%= workout.name %>
  </p>
 
  <p>
    <b>Description:</b>
    <%= workout.description %>
  </p>
  
  <p>
    <b>Rating:</b>
    <%= workout.rating %>
  </p>
<% end %>


<%= link_to 'Edit', edit_type_path(@type) %> |
<%= link_to 'Back', types_path %>
